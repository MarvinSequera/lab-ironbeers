<div class="col-md-2">
    <img class="img-size" src="{{image_url}}" alt="imagen">
</div>
<div class="col-md-8">
    <div class="column">
        <h1>{{name}}</h1>
        <h2>{{tagline}}</h2>
        <p>{{description}}</p>
        <p>Food Pairing</p>
        <ul>
            {{#each food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <p><b>Brewers Tips :</b>{{brewers_tips}}</p>
    </div>

</div>